{
  "name": "@react-page/renderer",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "module": "./lib-es/index.js",
  "typings": "./lib/index.d.ts",
  "author": "ORY GmbH",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "build": "npm-run-all --parallel build:lib",
    "build:watch": "npm-run-all --parallel build:lib:watch",
    "build:lib": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-es.json",
    "build:lib:watch": "npm run build:lib -- --watch",
    "docs": "documentation build ./lib/**/*.js --format md --github -o ../../docs/api/renderer.md",
    "prebuild": "npm run clean",
    "clean": "rimraf \"lib/*\""
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "@react-page/core": "0.1.0"
  },
  "devDependencies": {
    "@react-page/plugins-slate": "0.0.0",
    "@types/react": "16.7.17",
    "@types/slate": "^0.44.9",
    "@types/slate-react": "^0.21.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
